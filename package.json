{
  "name": "portfolio",
  "version": "0.0.0",
  "engines": {
    "node": "10.11.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "start:dev": "node app.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "wds": "webpack-dev-server --config webpack.server.config.js",
    "dev": "cross-env TZ=UTC NODE_ENV=development tsnd --inspect --respawn server.ts",
    "postinstall": "npm run build:ssr",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "ssr": "npm run build:ssr && npm run serve:ssr",
    "serve:ssr": "nodemon dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server && cp src/robots.txt dist/browser && cp src/sitemap.xml dist/browser",
    "build:client-and-server-bundles": "ng build --prod && ng run portfolio:server:production --bundleDependencies all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/cli": "^8.3.20",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@hasezoey/typegoose": "^5.9.2",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "bootstrap": "^4.4.1",
    "compression": "^1.7.4",
    "core-js": "^2.6.10",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "handlebars": "^4.5.3",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.14",
    "path": "^0.12.7",
    "popper.js": "^1.16.0",
    "rxjs": "^6.5.3",
    "webpack-node-externals": "^1.7.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.6",
    "@angular/language-service": "^8.2.14",
    "@types/compression": "0.0.36",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/mongoose": "^5.5.32",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.0",
    "cross-env": "^6.0.3",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-ts": "^5.0.3",
    "nodemon": "^1.19.4",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5",
    "webpack-cli": "^3.3.10"
  }
}
