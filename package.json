    "build:client-and-server-bundles_bak": "ng build --prod && ng run portfolio:server:production --bundleDependencies all",
    "dev:ssr": "ng run portfolio:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run portfolio:server:production",
    "prerender": "ng run portfolio:prerender"
  },
